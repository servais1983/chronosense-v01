{
  "ai": {
    "model_name": "microsoft/Phi-3-mini-4k-instruct",
    "mode": "simulation",
    "max_tokens": 512,
    "temperature": 0.7,
    "api_url": "http://localhost:11434/api/generate",
    "timeout": 60
  },
  "interface": {
    "window_size": "1200x800",
    "theme": "default",
    "font_size": 10,
    "graph_layout": "spring"
  },
  "graph": {
    "node_colors": {
      "ip": "#FF6B6B",
      "hash": "#4ECDC4",
      "file": "#45B7D1",
      "process": "#96CEB4",
      "domain": "#FFEAA7",
      "default": "#DDA0DD"
    },
    "node_size": 1000,
    "edge_width": 2
  },
  "logging": {
    "level": "INFO",
    "file": "logs/chronosense.log",
    "max_size": "10MB",
    "backup_count": 5
  }
}
